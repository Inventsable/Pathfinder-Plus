<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.live-pathfinder" ExtensionBundleVersion="1.0.0" ExtensionBundleName="live-pathfinder" Version="8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.live-pathfinder.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList><Host Name="ILST" Version="[23,99.9]" /></HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="8.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.live-pathfinder.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./public/index-dev.html</MainPath>
          <!-- <MainPath>./dist/index.html</MainPath> -->
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Pathfinder Plus</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>280</Width>
            </Size>
            <MaxSize>
              <Height>1000</Height>
              <Width>1000</Width>
            </MaxSize>
            <MinSize>
              <Height>300</Height>
              <Width>40</Width>
            </MinSize>
          </Geometry>
          <Icons>
             <Icon Type="Normal">./logoLight.png</Icon>
             <Icon Type="DarkNormal">./logoDark.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
